<% title = currentPage[0].replace("-", " "); %> 
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="../css/style.css" />
    <title><%= title %> | <%= clientName %></title>
  </head>
  <body>
    <header>
      <%- include('./partials/top-bar.ejs') %>
      <%- include('./partials/nav-items.ejs') %>
    </header>
    <main>
      <% pages.forEach(checkPage => { %>
        <% if (currentPage == checkPage) { %>
          <%- include(`./partials/content/${currentPage}.ejs`) %>
        <% } %>
      <% }) %>
    </main>
  </body>
</html>
